{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.13.6/.schema/devbox.schema.json",
  "packages": [
    "_1password@latest",
    "ack@latest",
    "bat@latest",
    "btop@latest",
    "coreutils@latest",
    "darwin.trash",
    "duckdb@latest",
    "ffmpeg@latest",
    "fzf@latest",
    "graphviz@latest",
    "jq@latest",
    "just@latest",
    "mosh@latest",
    "mtr@latest",
    "pv@latest",
    "ripgrep@latest",
    "starship@latest",
    "sqlite@latest",
    "tmux@latest",
    "unixtools.watch@latest",
    "wrk@latest",
    "xz@latest",
    "yt-dlp@latest",
    "curl@latest",
    "git@latest",
    "neovim@latest",
    "wget@latest",
    "fish@latest",
    "zellij@latest",
    "tokei@latest"
  ],
  "shell": {
    "init_hook": [
      "echo 'Welcome to devbox!' > /dev/null"
    ],
    "scripts": {
      "push": [
        "devbox global push git@github.com:quells/nix-darwin.git"
      ],
      "pull": [
        "devbox global pull git@github.com:quells/nix-darwin.git"
      ],
      "put_scripts": [
        "./dotfiles.sh"
      ]
    }
  }
}
